{
    "name": "alemaire/craft.backend.sf",
    "license": "proprietary",
    "type": "project",
    "config": {
        "platform": {
            "php": "7.2"
        },
        "discard-changes": true
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "beberlei/doctrineextensions": "^1.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "doctrine/orm": "^2.5",
        "easycorp/easyadmin-bundle": "^1.17",
        "ext-json": "*",
        "ext-mbstring": "*",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "friendsofsymfony/user-bundle": "~2.0.0",
        "gedmo/doctrine-extensions": "^2.4",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/doctrine-behaviors": "1.4",
        "league/uri": "^6.2",
        "league/uri-components": "^2.2",
        "liip/imagine-bundle": "^2.0",
        "marc-mabe/php-enum": "^2.2",
        "nelmio/cors-bundle": "1.5.6",
        "ocramius/proxy-manager": "^2.0.2",
        "php": "~7.2.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "sensio/generator-bundle": "^3.0",
        "sentry/sentry-symfony": "^3.0",
        "symfony/monolog-bundle": "^3.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/symfony": "3.4.*",
        "vich/uploader-bundle": "^1.7",
        "youthweb/urllinker": "^1.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "fix": [
            "php-cs-fixer fix"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
